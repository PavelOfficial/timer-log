{
  "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      ["@babel/preset-react", { "useBuiltIns": "entry", "runtime": "automatic" }]
  ],
  "plugins": [
      ["@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ],
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread",
      "babel-plugin-macros"
  ]
}